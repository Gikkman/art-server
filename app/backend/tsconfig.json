{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "incremental": true,
    "sourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2020"
    ],
    "outDir": "_dist"
  },
  "exclude": [
    "node_modules"
  ],
}